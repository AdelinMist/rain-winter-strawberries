<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <VBox alignment="CENTER" layoutX="14.0" layoutY="-13.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
              <Button fx:id="doctorAppointmentBtn" mnemonicParsing="false" onAction="#showDoctorAppointments" prefHeight="26.0" prefWidth="205.0" text="doctor appointments" />
              <Button fx:id="vaccineAppointmentBtn" mnemonicParsing="false" onAction="#showVaccineAppointments" prefHeight="26.0" prefWidth="205.0" text="vaccine appointments" />
              <Button fx:id="covidTestAppointmentBtn" mnemonicParsing="false" onAction="#showCovidTestAppointments" prefHeight="26.0" prefWidth="205.0" text="covid test appointments" />
            <Button fx:id="returnBtn" mnemonicParsing="false" onAction="#returnToMenu" prefHeight="26.0" prefWidth="205.0" text="return to menu" />
         </children>
      </VBox>
    </children>
</AnchorPane>
