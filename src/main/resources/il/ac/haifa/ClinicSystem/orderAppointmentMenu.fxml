<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <VBox alignment="CENTER" layoutX="192.0" layoutY="41.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
              <Button fx:id="doctorBTN" mnemonicParsing="false" onAction="#clickDoctor" prefHeight="26.0" prefWidth="205.0" text="doctor appointments" />
              <Button fx:id="vaccine" mnemonicParsing="false" onAction="#gotovac" prefHeight="26.0" prefWidth="205.0" text="vaccine appointments" />
             <Button fx:id="covid_test" mnemonicParsing="false" onAction="#gotoQuiz" prefHeight="26.0" prefWidth="205.0" text="covid test appointments" />
             <Button fx:id="family" mnemonicParsing="false" onAction="#goFamily" prefHeight="26.0" prefWidth="205.0" text="family/children appointments" />
             <Button fx:id="sister" mnemonicParsing="false" onAction="#goSister" prefHeight="26.0" prefWidth="205.0" text="sister appointments" />
              <Button fx:id="returnBtn" mnemonicParsing="false" onAction="#returnToMenu" prefHeight="26.0" prefWidth="205.0" text="return to menu" />
         </children>
      </VBox>
    </children>
</AnchorPane>
