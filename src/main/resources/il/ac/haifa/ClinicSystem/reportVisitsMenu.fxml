<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox alignment="CENTER" layoutX="140.0" layoutY="67.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Button fx:id="vaccineBtn" mnemonicParsing="false" onAction="#showVaccineVisits" prefHeight="26.0" prefWidth="205.0" text="Vaccine Visits" />
                <Button fx:id="covidTestBtn" mnemonicParsing="false" onAction="#showCovidTestVisits" prefHeight="26.0" prefWidth="205.0" text="Covid Test Visits" />
                <Button fx:id="familyBtn" layoutX="10.0" layoutY="36.0" mnemonicParsing="false" onAction="#showFamilyVisits" prefHeight="26.0" prefWidth="205.0" text="Family Doctor Visits" />
            <Button fx:id="pediatricBtn" layoutX="10.0" layoutY="62.0" mnemonicParsing="false" onAction="#showPediatricVisits" prefHeight="26.0" prefWidth="205.0" text="Pediatric Doctor Visits" />
            <Button fx:id="professionalBtn" layoutX="10.0" layoutY="88.0" mnemonicParsing="false" onAction="#showProfessionalVisits" prefHeight="26.0" prefWidth="205.0" text="Professional Doctor Visits" />
            <Button fx:id="nurseBtn" layoutX="10.0" layoutY="114.0" mnemonicParsing="false" onAction="#showNurseVisits" prefHeight="26.0" prefWidth="205.0" text="Nurse Visits" />
            <Button fx:id="labBtn" layoutX="10.0" layoutY="140.0" mnemonicParsing="false" onAction="#showLabVisits" prefHeight="26.0" prefWidth="205.0" text="Lab Visits" />
                <Button fx:id="returnBtn" mnemonicParsing="false" onAction="#returnToMenu" prefHeight="26.0" prefWidth="205.0" text="Return to Menu" />
            </children>
        </VBox>
    </children>
</AnchorPane>
