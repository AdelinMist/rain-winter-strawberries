<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="borderPane" prefHeight="703.0" prefWidth="1214.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <Label fx:id="listLbl" alignment="CENTER" prefHeight="24.0" prefWidth="1214.0" text="Doctor Wait Times Report" textAlignment="CENTER" BorderPane.alignment="CENTER">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Label>
    </top>
    <center>
        <TableView fx:id="reportTable" prefHeight="533.0" prefWidth="1214.0" BorderPane.alignment="CENTER">
            <columns>
                <TableColumn fx:id="date" prefWidth="165.0" text="Date of Creation" />
            <TableColumn fx:id="doctor" prefWidth="187.0" text="Doctor" />
                <TableColumn fx:id="sunday" prefWidth="130.5" text="Sunday" />
                <TableColumn fx:id="monday" prefWidth="137.5" text="Monday" />
                <TableColumn fx:id="tuesday" prefWidth="138.5" text="Tuesday" />
                <TableColumn fx:id="wednesday" prefWidth="157.0" text="Wednesday" />
                <TableColumn fx:id="thursday" maxWidth="176.0" prefWidth="150.0" text="Thursday" />
                <TableColumn fx:id="friday" prefWidth="147.5" text="Friday" />
            </columns>
        </TableView>
    </center>
    <bottom>
        <AnchorPane prefHeight="39.0" prefWidth="1214.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="returnBtn" layoutX="14.0" layoutY="7.0" mnemonicParsing="false" onAction="#returnToMenu" text="Return to Menu" />
            </children>
        </AnchorPane>
    </bottom>
</BorderPane>
