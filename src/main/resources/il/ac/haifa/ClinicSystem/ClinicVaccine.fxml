<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane fx:id="borderPane" prefHeight="703.0" prefWidth="1214.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <Label fx:id="listLbl" alignment="CENTER" prefHeight="24.0" prefWidth="1214.0" text="vaccine clinics List" textAlignment="CENTER" BorderPane.alignment="CENTER">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Label>
    </top>
    <center>
        <TableView fx:id="vaccineClinicTable" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="20.0" minWidth="20.0" prefHeight="417.0" prefWidth="1214.0" BorderPane.alignment="TOP_CENTER">
            <columns>
                <TableColumn fx:id="name" prefWidth="620.0" text="Clinic Name" />
                <TableColumn fx:id="place" prefWidth="593.0" text="Location" />

            </columns>
        </TableView>
    </center>
    <bottom>
        <AnchorPane prefHeight="123.0" prefWidth="1214.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="returnBtn" layoutX="69.0" layoutY="77.0" mnemonicParsing="false" onAction="#returnToMenu" text="Return to Menu" />
                <Button fx:id="addBtn" layoutX="1050.0" layoutY="77.0" mnemonicParsing="false" onAction="#next_page" text="next" />
                <DatePicker fx:id="datePicker" layoutX="354.0" layoutY="37.0" />
                <ChoiceBox fx:id="AppointmentTimeChoiceBox" layoutX="681.0" layoutY="37.0" prefWidth="150.0" />
                <Text layoutX="380.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Pick Appointment Date" />
                <Text layoutX="695.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Pick Appointment time" />
            </children>
        </AnchorPane>
    </bottom>
</BorderPane>
