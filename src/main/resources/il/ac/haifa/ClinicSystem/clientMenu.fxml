<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" layoutX="140.0" layoutY="67.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button fx:id="viewBtn" mnemonicParsing="false" onAction="#showViewMenu" text="view appointments" />
            <Button fx:id="orderBtn" mnemonicParsing="false" onAction="#showOrderMenu" text="order appointments" />
            <Button fx:id="greenBtn" mnemonicParsing="false" onAction="#makeGreenPass" text="green pass " />
            <Button fx:id="exitBtn" mnemonicParsing="false" onAction="#exit" text="exit" />
         </children>
      </VBox>
   </children>
</AnchorPane>
